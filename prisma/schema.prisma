// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
}

model Partner {
  id Int @id @default(autoincrement())
  name String
  company_form String
  tax_number String
  registration_number String
  settlement String
  address String
  phone String
  account_number String
  note String
}